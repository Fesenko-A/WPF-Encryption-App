<Window x:Class="VolvoApp.MessageBoxes.AccessDeniedWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VolvoApp.MessageBoxes"
        mc:Ignorable="d"
        Title="{DynamicResource StrAccessDenied}" Height="290" Width="515" ResizeMode="NoResize" FontSize="15pt">
    <Grid>
        <TextBlock TextAlignment="Center" HorizontalAlignment="Center" Margin="0,82,0,0" TextWrapping="Wrap" Text="{DynamicResource StrAccessDenied}" VerticalAlignment="Top" Width="332"/>
        <Button IsDefault="True" Content="OK" HorizontalAlignment="Center" Margin="0,154,0,0" VerticalAlignment="Top" Height="51" Width="95" Click="Button_Click"/>
    </Grid>
</Window>
