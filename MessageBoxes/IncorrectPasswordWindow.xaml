<Window x:Class="VolvoApp.MessageBoxes.IncorrectPasswordWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VolvoApp.MessageBoxes"
        mc:Ignorable="d"
        Title="{DynamicResource StrIncorrectPassword}" Height="290" Width="515" ResizeMode="NoResize" FontSize="15pt">
    <Grid>
        <TextBlock TextAlignment="Center" HorizontalAlignment="Center" Margin="0,66,0,0" TextWrapping="Wrap" Text="{DynamicResource StrIncorrectPassword}" VerticalAlignment="Top" Width="390" Height="53"/>
        <Button IsDefault="True" Name="OkButton" Content="OK" HorizontalAlignment="Center" Margin="0,174,0,0" VerticalAlignment="Top" Height="50" Width="130" Click="OkButton_Click"/>
    </Grid>
</Window>
